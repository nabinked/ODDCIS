<http://isit990.azurewebsites.net/ontology/dementia-info> rdf:type owl:Ontology ;
                                                           rdfs:comment "An ontology for a information retrieval system developed by the student of UOW, Australia as part of thrie ISIT 990 final year project."^^rdfs:Literal .

#################################################################
#    Object Properties
#################################################################

###  http://isit990.azurewebsites.net/ontology/dementia-info/associatedWith
:associatedWith rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Symptom ;
                rdfs:range :Dementia ,
                           :TypesOfDementia ;
                rdfs:label "associated with" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/consistingOf
:consistingOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Symptom ;
              rdfs:range :Behavior ,
                         :Confusion ,
                         :DeclineInCognition ,
                         :Dizziness ,
                         :Hallucination ,
                         :Headache ,
                         :MemoryLoss ,
                         :Tiredness ;
              rdfs:label "consisting of" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/for
:for rdf:type owl:ObjectProperty ;
     rdfs:subPropertyOf owl:topObjectProperty ;
     rdfs:domain :PreventionMethod ,
                 :Symptom ,
                 :Treatment ;
     rdfs:range :PhaseOfDementia ,
                :TypesOfDementia ;
     rdfs:label "for" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/hasBehaviour
:hasBehaviour rdf:type owl:ObjectProperty ;
              rdfs:domain :Symptom ;
              rdfs:range :Behavior ;
              rdfs:label "has behaviour" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/hasDefinitionOf
:hasDefinitionOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :Webpage ;
                 rdfs:range :Dementia ;
                 rdfs:label "has definition of" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/hasWebpage
:hasWebpage rdf:type owl:ObjectProperty ;
            rdfs:domain :WebSite ;
            rdfs:range :Webpage ;
            rdfs:label "has webpage" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/of
:of rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain :Diagnosis ,
                :Impact ,
                :PreventionMethod ,
                :Prognosis ,
                :RiskFactors ,
                :Symptom ,
                :Treatment ;
    rdfs:range :PhaseOfDementia ,
               :TypesOfDementia ;
    rdfs:label "of" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/providedTo
:providedTo rdf:type owl:ObjectProperty ;
            rdfs:domain :FinancialAssistance ;
            rdfs:label "provided to" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/relatedTo
:relatedTo rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :RiskFactors ;
           rdfs:range :Dementia ,
                      :sDisease ;
           rdfs:label "related to" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/usedFor
:usedFor rdf:type owl:ObjectProperty ;
         rdfs:domain :Treatment ;
         rdfs:range :TypesOfDementia ;
         rdfs:label "used for" .


#################################################################
#    Data properties
#################################################################

###  http://isit990.azurewebsites.net/ontology/dementia-info/hasExcerpt
:hasExcerpt rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Webpage ;
            rdfs:range rdf:PlainLiteral ;
            rdfs:label "has excerpt" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/hasTitle
:hasTitle rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :Webpage ;
          rdfs:range rdf:PlainLiteral ;
          rdfs:label "has title" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/hasUrl
:hasUrl rdf:type owl:DatatypeProperty ;
        rdfs:domain :Webpage ;
        rdfs:range rdf:PlainLiteral ;
        rdfs:label "has url address" .


