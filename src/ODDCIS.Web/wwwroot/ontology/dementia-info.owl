<http://isit990.azurewebsites.net/ontology/dementia-info> rdf:type owl:Ontology ;
                                                           rdfs:comment "An ontology for a information retrieval system developed by the student of UOW, Australia as part of thrie ISIT 990 final year project."^^rdfs:Literal .

#################################################################
#    Object Properties
#################################################################

###  http://isit990.azurewebsites.net/ontology/dementia-info/associatedWith
:associatedWith rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Symptom ;
                rdfs:range :Dementia ,
                           :TypesOfDementia ;
                rdfs:label "associated with" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/for
:for rdf:type owl:ObjectProperty ;
     rdfs:subPropertyOf owl:topObjectProperty ;
     rdfs:domain :PreventionMethod ,
                 :Symptom ,
                 :Treatment ;
     rdfs:range :PhaseOfDementia ,
                :TypesOfDementia ;
     rdfs:label "for" .


###  http://isit990.azurewebsites.net/ontology/dementia-info/hasBehaviour
:hasBehaviour rdf:type owl:ObjectProperty ;
              rdfs:domain :Symptom .


###  http://isit990.azurewebsites.net/ontology/dementia-info/hasDefinitionOf
:hasDefinitionOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :Webpage ;
                 rdfs:range :Dementia .


###  http://isit990.azurewebsites.net/ontology/dementia-info/hasWebpage
:hasWebpage rdf:type owl:ObjectProperty ;
            rdfs:domain :WebSite ;
            rdfs:range :Webpage .


###  http://isit990.azurewebsites.net/ontology/dementia-info/of
:of rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain :Diagnosis ,
                :Impact ,
                :PreventionMethod ,
                :Prognosis ,
                :RiskFactors ,
                :Symptom ,
                :Treatment ;
    rdfs:range :PhaseOfDementia ,
               :TypesOfDementia ;
    rdfs:label "of" .


#################################################################
#    Data properties
#################################################################

###  http://isit990.azurewebsites.net/ontology/dementia-info/hasExcerpt
:hasExcerpt rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Webpage ;
            rdfs:range rdf:PlainLiteral .


###  http://isit990.azurewebsites.net/ontology/dementia-info/hasTitle
:hasTitle rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :Webpage ;
          rdfs:range rdf:PlainLiteral .


###  http://isit990.azurewebsites.net/ontology/dementia-info/hasUrl
:hasUrl rdf:type owl:DatatypeProperty ;
        rdfs:domain :Webpage ;
        rdfs:range rdf:PlainLiteral ;
        rdfs:label "hasUrlAddress"@en .


